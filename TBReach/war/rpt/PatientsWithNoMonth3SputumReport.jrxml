				<text><![CDATA[CHWID]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column header" x="237" y="26" width="79" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[GPID]]></text>
