	</style>
	<parameter name="UserName" class="java.lang.String"/>
	<queryString>
		<![CDATA[select A.PatientID, A.MRNo, A.CHWID, A.GPID, A.Category, A.DateBaselineVisit, (select count(EncounterID) from Encounter where PID1 = A.PatientID and EncounterType = 'SPUTUM_COL' and datediff(DateEncounterEntered, DateBaselineVisit) between 84 and 113) as SputumAttempts
from (select * from _ConfirmedTBPatients P where exists (select * from SputumResults where PatientID = P.PatientID and datediff(curdate(), DateBaselineVisit) between 84 and 113)) A
where A.Category = 'CAT II' and Diagnosis <> 'EP-TB'
and not exists (select * from SputumResults where PatientID = A.PatientID and Remarks not like 'REJECTED%' and datediff(DateSubmitted, A.DateBaselineVisit) between 84 and 113);
]]>
	</queryString>
	<field name="PatientID" class="java.lang.String"/>
	<field name="MRNo" class="java.lang.String"/>
	<field name="CHWID" class="java.lang.String"/>
	<field name="GPID" class="java.lang.String"/>
	<field name="Category" class="java.lang.String"/>
	<field name="DateBaselineVisit" class="java.sql.Timestamp"/>
	<field name="SputumAttempts" class="java.lang.Long"/>
	<variable name="CURRENT_DATE" class="java.lang.String" calculation="System"/>
