		<defaultValueExpression><![CDATA[new java.util.Date()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
EncounterType,
     COUNT(*) AS Total
FROM Encounter
WHERE
     DateEncounterStart BETWEEN $P{StartDate} AND $P{EndDate}
GROUP BY
     EncounterType]]>
	</queryString>
	<field name="EncounterType" class="java.lang.String"/>
	<field name="Total" class="java.lang.Long"/>
