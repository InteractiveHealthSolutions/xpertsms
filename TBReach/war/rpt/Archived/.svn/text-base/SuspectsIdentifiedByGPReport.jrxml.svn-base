<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="ReportFormat" language="groovy" pageWidth="595" pageHeight="842" whenNoDataType="AllSectionsNoDetail" columnWidth="535" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="Title" forecolor="#FFFFFF" fontName="SansSerif" fontSize="26" isBold="true" pdfFontName="Helvetica-Bold"/>
	<style name="SubTitle" forecolor="#666666" fontName="SansSerif" fontSize="18"/>
	<style name="Detail" mode="Transparent" forecolor="#010101" backcolor="#FFFFFF" fill="Solid" fontName="SansSerif" fontSize="8" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="false">
		<box topPadding="2" leftPadding="2" bottomPadding="2" rightPadding="2">
			<pen lineWidth="0.5" lineColor="#000000"/>
			<topPen lineWidth="0.5" lineColor="#000000"/>
			<leftPen lineWidth="0.5" lineColor="#000000"/>
			<bottomPen lineWidth="0.5" lineColor="#000000"/>
			<rightPen lineWidth="0.5" lineColor="#000000"/>
		</box>
	</style>
	<style name="Border" fill="Solid" fontName="SansSerif" fontSize="9">
		<box topPadding="2" leftPadding="2">
			<pen lineWidth="0.5" lineColor="#000000"/>
			<topPen lineWidth="0.5" lineColor="#000000"/>
			<leftPen lineWidth="0.5" lineColor="#000000"/>
			<bottomPen lineWidth="0.5" lineColor="#000000"/>
			<rightPen lineWidth="0.5" lineColor="#000000"/>
		</box>
	</style>
	<style name="Column header" mode="Transparent" forecolor="#010101" fontName="SansSerif" fontSize="9" isBold="true">
		<box topPadding="2" leftPadding="2" bottomPadding="2" rightPadding="2">
			<pen lineWidth="1.0" lineColor="#333333"/>
			<topPen lineWidth="1.0" lineColor="#333333"/>
			<leftPen lineWidth="1.0" lineColor="#333333"/>
			<bottomPen lineWidth="1.0" lineColor="#333333"/>
			<rightPen lineWidth="1.0" lineColor="#333333"/>
		</box>
	</style>
	<parameter name="UserName" class="java.lang.String"/>
	<parameter name="GPID" class="java.lang.String">
		<defaultValueExpression><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT P.GPID, WEEK(P.DateRegistered) AS WeekOfRegistration, COUNT(*) AS Total, (SELECT COUNT(*) FROM tbreach_rpt.Patient Pt WHERE Pt.GPID = P.GPID AND WEEK(Pt.DateRegistered) <= WEEK(P.DateRegistered)) AS CumulativeTotal FROM tbreach_rpt.Patient P
GROUP BY GPID, WEEK(DateRegistered);]]>
	</queryString>
	<field name="GPID" class="java.lang.String"/>
	<field name="WeekOfRegistration" class="java.lang.Integer"/>
	<field name="Total" class="java.lang.Long"/>
	<field name="CumulativeTotal" class="java.lang.Long"/>
	<sortField name="GPID"/>
	<variable name="CURRENT_DATE" class="java.lang.String" calculation="System"/>
	<group name="GP">
		<groupExpression><![CDATA[$F{GPID}]]></groupExpression>
		<groupFooter>
			<band height="322">
				<lineChart>
					<chart>
						<reportElement x="0" y="0" width="555" height="321"/>
						<chartTitle/>
						<chartSubtitle/>
						<chartLegend position="Top"/>
					</chart>
					<categoryDataset>
						<dataset resetType="Group" resetGroup="GP"/>
						<categorySeries>
							<seriesExpression><![CDATA[$F{GPID}]]></seriesExpression>
							<categoryExpression><![CDATA[$F{WeekOfRegistration}]]></categoryExpression>
							<valueExpression><![CDATA[$F{CumulativeTotal}]]></valueExpression>
						</categorySeries>
					</categoryDataset>
					<linePlot>
						<plot/>
						<categoryAxisLabelExpression><![CDATA["Week of Registration"]]></categoryAxisLabelExpression>
						<categoryAxisFormat>
							<axisFormat>
								<labelFont/>
								<tickLabelFont/>
							</axisFormat>
						</categoryAxisFormat>
						<valueAxisLabelExpression><![CDATA["Suspects"]]></valueAxisLabelExpression>
						<valueAxisFormat>
							<axisFormat>
								<labelFont/>
								<tickLabelFont/>
							</axisFormat>
						</valueAxisFormat>
					</linePlot>
				</lineChart>
			</band>
		</groupFooter>
	</group>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band height="51" splitType="Stretch">
			<staticText>
				<reportElement x="50" y="0" width="505" height="40"/>
				<textElement textAlignment="Center">
					<font size="22"/>
				</textElement>
				<text><![CDATA[SUSPECTS IDENTIFIED BY GP (CUMULATIVE)]]></text>
			</staticText>
			<image>
				<reportElement x="0" y="0" width="50" height="50"/>
				<imageExpression class="java.lang.String"><![CDATA["indusLogo.jpg"]]></imageExpression>
			</image>
		</band>
	</title>
	<pageHeader>
		<band height="14">
			<textField evaluationTime="Report">
				<reportElement x="513" y="0" width="42" height="13" forecolor="#7B7B7B"/>
				<textElement verticalAlignment="Middle">
					<font size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[" " + $V{PAGE_NUMBER}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="433" y="0" width="80" height="13" forecolor="#7B7B7B"/>
				<textElement textAlignment="Right" verticalAlignment="Middle">
					<font size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Page "+$V{PAGE_NUMBER}+" of"]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	<lastPageFooter>
		<band height="14">
			<staticText>
				<reportElement x="0" y="0" width="67" height="13" forecolor="#7B7B7B"/>
				<textElement>
					<font fontName="SansSerif" isBold="true"/>
				</textElement>
				<text><![CDATA[Printed By:]]></text>
			</staticText>
			<textField pattern="dd-MMM-yy h:mm a">
				<reportElement x="441" y="0" width="114" height="13" forecolor="#7B7B7B"/>
				<textElement verticalAlignment="Middle">
					<font size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.util.Date"><![CDATA[new java.util.Date()]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="67" y="0" width="75" height="13" forecolor="#7B7B7B"/>
				<textElement>
					<font fontName="SansSerif" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{UserName}]]></textFieldExpression>
			</textField>
		</band>
	</lastPageFooter>
</jasperReport>
