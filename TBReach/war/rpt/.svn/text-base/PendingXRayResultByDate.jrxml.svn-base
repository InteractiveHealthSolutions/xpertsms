			<leftPen lineWidth="0.5" lineColor="#000000"/>
			<bottomPen lineWidth="0.5" lineColor="#000000"/>
			<rightPen lineWidth="0.5" lineColor="#000000"/>
		</box>
	</style>
	<parameter name="UserName" class="java.lang.String"/>
	<parameter name="StartDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[new java.util.Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="EndDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[new java.util.Date()]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT X.PatientID, Pt.MRNo, CONCAT(IFNULL(P.FirstName, ''), ' ', IFNULL(P.LastName, '')) AS PatientName, X.IRS, X.XRayDate, X.XRayResults, X.Remarks FROM tbreach_rpt.XRayResults X
INNER JOIN tbreach_rpt.Person P ON P.PID = X.PatientID
INNER JOIN tbreach_rpt.Patient Pt ON Pt.PatientID = X.PatientID
WHERE DateReported IS NULL AND XRayResults = '' AND X.XRayDate BETWEEN $P{StartDate} AND $P{EndDate};]]>
	</queryString>
	<field name="PatientID" class="java.lang.String"/>
	<field name="MRNo" class="java.lang.String"/>
	<field name="PatientName" class="java.lang.String"/>
	<field name="IRS" class="java.lang.Integer"/>
	<field name="XRayDate" class="java.sql.Timestamp"/>
	<field name="XRayResults" class="java.lang.String"/>
	<field name="Remarks" class="java.lang.String"/>
	<sortField name="XRayDate"/>
	<variable name="CURRENT_DATE" class="java.lang.String" calculation="System"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band height="21" splitType="Stretch">
			<staticText>
				<reportElement style="Column header" x="0" y="0" width="62" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[PatientID]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column header" x="62" y="0" width="62" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[MRNo]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column header" x="124" y="0" width="113" height="20"/>
				<textElement/>
				<text><![CDATA[Patient Name]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column header" x="237" y="0" width="67" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[IRS]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column header" x="304" y="0" width="58" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[XRay Date]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column header" x="362" y="0" width="91" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[XRay Results]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column header" x="453" y="0" width="102" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[Remarks]]></text>
			</staticText>
		</band>
	</title>
	<detail>
		<band height="20">
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="0" y="0" width="62" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{PatientID}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="124" y="0" width="113" height="20"/>
				<textElement/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{PatientName}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="237" y="0" width="67" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{IRS}]]></textFieldExpression>
			</textField>
			<textField pattern="dd-MMM-yy" isBlankWhenNull="true">
				<reportElement style="Detail" x="304" y="0" width="58" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.sql.Timestamp"><![CDATA[$F{XRayDate}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="362" y="0" width="91" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{XRayResults}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="453" y="0" width="102" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{Remarks}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="62" y="0" width="62" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{MRNo}]]></textFieldExpression>
			</textField>
