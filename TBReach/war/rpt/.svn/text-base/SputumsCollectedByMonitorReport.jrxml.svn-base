<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="ReportFormat" language="groovy" pageWidth="595" pageHeight="842" whenNoDataType="AllSectionsNoDetail" columnWidth="535" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="Title" forecolor="#FFFFFF" fontName="SansSerif" fontSize="26" isBold="true" pdfFontName="Helvetica-Bold"/>
	<style name="SubTitle" forecolor="#666666" fontName="SansSerif" fontSize="18"/>
	<style name="Column header" mode="Transparent" forecolor="#666666" fontName="SansSerif" fontSize="12" isBold="true">
		<box>
			<pen lineWidth="0.0" lineColor="#333333"/>
			<topPen lineWidth="0.0" lineColor="#333333"/>
			<leftPen lineWidth="0.0" lineColor="#333333"/>
			<bottomPen lineWidth="0.0" lineColor="#333333"/>
			<rightPen lineWidth="0.0" lineColor="#333333"/>
		</box>
	</style>
	<style name="Detail" mode="Transparent" forecolor="#000000" backcolor="#FFFFFF" fill="Solid" fontName="SansSerif" fontSize="10" isBold="false" isItalic="false" isUnderline="false" isStrikeThrough="false">
		<box topPadding="2" leftPadding="2" bottomPadding="2" rightPadding="2">
			<pen lineWidth="0.5" lineColor="#000000"/>
			<topPen lineWidth="0.5" lineColor="#000000"/>
			<leftPen lineWidth="0.5" lineColor="#000000"/>
			<bottomPen lineWidth="0.5" lineColor="#000000"/>
			<rightPen lineWidth="0.5" lineColor="#000000"/>
		</box>
	</style>
	<style name="Border" fill="Solid" fontName="SansSerif" fontSize="9">
		<box topPadding="2" leftPadding="2">
			<pen lineWidth="0.5" lineColor="#000000"/>
			<topPen lineWidth="0.5" lineColor="#000000"/>
			<leftPen lineWidth="0.5" lineColor="#000000"/>
			<bottomPen lineWidth="0.5" lineColor="#000000"/>
			<rightPen lineWidth="0.5" lineColor="#000000"/>
		</box>
	</style>
	<style name="Column" fontName="SansSerif" fontSize="11" isBold="true" isItalic="false" isUnderline="true" isStrikeThrough="false">
		<box topPadding="2" leftPadding="2" bottomPadding="2" rightPadding="2">
			<pen lineWidth="0.5"/>
			<topPen lineWidth="0.5"/>
			<leftPen lineWidth="0.5"/>
			<bottomPen lineWidth="0.5"/>
			<rightPen lineWidth="0.5"/>
		</box>
	</style>
	<parameter name="UserName" class="java.lang.String"/>
	<parameter name="MonitorID" class="java.lang.String">
		<defaultValueExpression><![CDATA["%"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT MonitorID,
(SELECT COUNT(*) FROM tbreach_rpt.Patient WHERE PatientID IN (SELECT PatientID FROM tbreach_rpt.SputumResults) AND MonitorID = Monitor.MonitorID) AS Total,
(SELECT COUNT(*) FROM tbreach_rpt.Patient WHERE PatientID IN (SELECT DISTINCT PatientID FROM tbreach_rpt.SputumResults) AND PatientID NOT IN (SELECT DISTINCT PatientID FROM tbreach_rpt.SputumResults WHERE PatientID = Patient.PatientID AND Remarks NOT LIKE 'REJECTED%') AND MonitorID = Monitor.MonitorID) AS NoConfirmedSample,
(SELECT COUNT(*) FROM tbreach_rpt.Patient WHERE PatientID IN (SELECT DISTINCT PatientID FROM tbreach_rpt.SputumResults) AND PatientID IN  (SELECT DISTINCT PatientID FROM tbreach_rpt.SputumResults WHERE PatientID = Patient.PatientID AND Remarks NOT LIKE 'REJECTED%') AND MonitorID = Monitor.MonitorID) AS ConfirmedSample
FROM tbreach_rpt.Monitor
HAVING Total > 0;]]>
	</queryString>
	<field name="MonitorID" class="java.lang.String"/>
	<field name="Total" class="java.lang.Long"/>
	<field name="NoConfirmedSample" class="java.lang.Long"/>
	<field name="ConfirmedSample" class="java.lang.Long"/>
	<sortField name="MonitorID"/>
	<variable name="CURRENT_DATE" class="java.lang.String" calculation="System"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band height="51" splitType="Stretch">
			<staticText>
				<reportElement x="50" y="0" width="429" height="37"/>
				<textElement textAlignment="Center">
					<font size="24"/>
				</textElement>
				<text><![CDATA[SPUTUMS COLLECTED BY MONITOR]]></text>
			</staticText>
			<image>
				<reportElement x="0" y="0" width="50" height="50"/>
				<imageExpression class="java.lang.String"><![CDATA["indusLogo.jpg"]]></imageExpression>
			</image>
		</band>
	</title>
	<pageHeader>
		<band height="13">
			<textField>
				<reportElement style="Column header" x="433" y="0" width="80" height="13" forecolor="#7B7B7B"/>
				<textElement textAlignment="Right" verticalAlignment="Middle">
					<font size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["Page "+$V{PAGE_NUMBER}+" of"]]></textFieldExpression>
			</textField>
			<textField evaluationTime="Report">
				<reportElement style="Column header" x="513" y="0" width="42" height="13" forecolor="#7B7B7B"/>
				<textElement verticalAlignment="Middle">
					<font size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[" " + $V{PAGE_NUMBER}]]></textFieldExpression>
			</textField>
		</band>
	</pageHeader>
	<columnHeader>
		<band height="20">
			<staticText>
				<reportElement style="Column" x="105" y="0" width="80" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[Total Patients]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column" x="185" y="0" width="185" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[Patients with No Accepted Sample]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column" x="370" y="0" width="185" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[Patients with 1+ Accepted Sample(s)]]></text>
			</staticText>
			<staticText>
				<reportElement style="Column" x="0" y="0" width="105" height="20"/>
				<textElement textAlignment="Center"/>
				<text><![CDATA[Monitor ID]]></text>
			</staticText>
		</band>
	</columnHeader>
	<detail>
		<band height="20">
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="105" y="0" width="80" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.Long"><![CDATA[$F{Total}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="185" y="0" width="185" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.Long"><![CDATA[$F{NoConfirmedSample}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="370" y="0" width="185" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.Long"><![CDATA[$F{ConfirmedSample}]]></textFieldExpression>
			</textField>
			<textField isBlankWhenNull="true">
				<reportElement style="Detail" x="0" y="0" width="105" height="20"/>
				<textElement textAlignment="Center"/>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{MonitorID}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
	<columnFooter>
		<band height="251">
			<stackedBarChart>
				<chart theme="aegean">
					<reportElement x="0" y="0" width="555" height="251"/>
					<chartTitle/>
					<chartSubtitle/>
					<chartLegend position="Bottom"/>
				</chart>
				<categoryDataset>
					<categorySeries>
						<seriesExpression><![CDATA["None Accepted"]]></seriesExpression>
						<categoryExpression><![CDATA[$F{MonitorID}]]></categoryExpression>
						<valueExpression><![CDATA[$F{NoConfirmedSample}]]></valueExpression>
					</categorySeries>
					<categorySeries>
						<seriesExpression><![CDATA["1+ Accepted"]]></seriesExpression>
						<categoryExpression><![CDATA[$F{MonitorID}]]></categoryExpression>
						<valueExpression><![CDATA[$F{ConfirmedSample}]]></valueExpression>
					</categorySeries>
				</categoryDataset>
				<barPlot>
					<plot/>
					<itemLabel color="#000000" backgroundColor="#FFFFFF"/>
					<categoryAxisFormat>
						<axisFormat>
							<labelFont>
								<font fontName="SansSerif" size="8"/>
							</labelFont>
							<tickLabelFont>
								<font fontName="SansSerif" size="8"/>
							</tickLabelFont>
						</axisFormat>
					</categoryAxisFormat>
					<valueAxisFormat>
						<axisFormat>
							<labelFont>
								<font fontName="SansSerif" size="8"/>
							</labelFont>
							<tickLabelFont>
								<font fontName="SansSerif" size="8" pdfFontName="Helvetica"/>
							</tickLabelFont>
						</axisFormat>
					</valueAxisFormat>
				</barPlot>
			</stackedBarChart>
		</band>
	</columnFooter>
	<lastPageFooter>
		<band height="14">
			<staticText>
				<reportElement x="0" y="0" width="67" height="13" forecolor="#7B7B7B"/>
				<textElement>
					<font fontName="SansSerif" isBold="true"/>
				</textElement>
				<text><![CDATA[Printed By:]]></text>
			</staticText>
			<textField pattern="dd-MMM-yy h:mm a">
				<reportElement x="441" y="0" width="114" height="13" forecolor="#7B7B7B"/>
				<textElement verticalAlignment="Middle">
					<font size="10" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.util.Date"><![CDATA[new java.util.Date()]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="67" y="0" width="75" height="13" forecolor="#7B7B7B"/>
				<textElement>
					<font fontName="SansSerif" isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$P{UserName}]]></textFieldExpression>
			</textField>
		</band>
	</lastPageFooter>
</jasperReport>
