	<parameter name="UserName" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT L.UserID, DATE(L.DateChanged) AS DateUpdated, SUBSTRING(REPLACE(L.NewValue, 'P', '0'), 9, 11) AS PatientID, P.MRNo FROM Log_DataChange L
LEFT OUTER JOIN tbreach_rpt.Patient P ON P.PatientID = SUBSTRING(REPLACE(L.NewValue, 'P', '0'), 9, 11)
WHERE L.Entity = 'XRayResults' AND L.UserID <> ''
ORDER BY L.UserID;]]>
	</queryString>
	<field name="UserID" class="java.lang.String"/>
	<field name="DateUpdated" class="java.sql.Date"/>
	<field name="PatientID" class="java.lang.String"/>
	<field name="MRNo" class="java.lang.String"/>
	<variable name="CURRENT_DATE" class="java.lang.String" calculation="System"/>
	<variable name="UserID_1" class="java.lang.Integer" resetType="Group" resetGroup="UserID" calculation="DistinctCount">
		<variableExpression><![CDATA[$F{UserID}]]></variableExpression>
	</variable>
	<variable name="PatientID_1" class="java.lang.Integer" resetType="Group" resetGroup="UserID" calculation="DistinctCount">
		<variableExpression><![CDATA[$F{PatientID}]]></variableExpression>
	</variable>
	<group name="UserID">
