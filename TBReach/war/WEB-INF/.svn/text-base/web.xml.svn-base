<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>org.irdresearch.tbreach.server.ServerServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/tbreach/greet</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
		<servlet-name>mobileService</servlet-name>
		<servlet-class>org.irdresearch.tbreach.server.MobileService</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>mobileService</servlet-name>
		<url-pattern>/mobileservice</url-pattern>
	
	</servlet-mapping>

	
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/tbreach/remote</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>TBReach.html</welcome-file>
  </welcome-file-list>

</web-app>
