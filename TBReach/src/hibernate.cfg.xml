  <mapping resource="org/irdresearch/tbreach/shared/model/Worker.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/XrayResults.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/OtherMessageSetting.hbm.xml"/>
 </session-factory>
</hibernate-configuration>
