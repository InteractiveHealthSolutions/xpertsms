<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
                                         "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory name="">
  <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
  <property name="hibernate.connection.username">root</property>
  <property name="hibernate.connection.password">root</property>
  <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/xpertsms?zeroDateTimeBehavior=convertToNull</property>
  <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
  <property name="hibernate.connection.pool_size">0</property>
  <property name="hibernate.connection.autocommit">true</property>
  <property name="hibernate.show_sql">true</property>
<!--   <property name="hibernate.hbm2ddl.auto">update</property> -->
  <property name="hibernate.use_sql_comments">false</property>
  <property name="hibernate.format_sql">false</property>
  <property name="hibernate.transaction.auto_close_session">true</property>
  <mapping resource="org/irdresearch/tbreach/shared/model/Contact.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/Encounter.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/EncounterResults.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/GeneXpertResults.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/GeneXpertResultsUnlinked.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/Gp.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/Location.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/LogDataChange.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/LogDataDelete.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/LogDataInsert.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/LogUserLogin.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/MessageSettings.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/Patient.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/Person.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/SetupCity.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/SetupCountry.hbm.xml"/>
  <mapping resource="org/irdresearch/tbreach/shared/model/SputumResults.hbm.xml"/>
